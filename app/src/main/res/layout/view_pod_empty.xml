<?xml version="1.0" encoding="utf-8"?>
<com.example.waiyan.mmhealthkotlin.viewpods.EmptyViewPod xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent" android:layout_height="match_parent">
    <ImageView
        android:id="@+id/iv_empty"
        android:layout_width="200dp"
        android:layout_height="200dp"
        android:src="@drawable/empty_data"
        android:layout_centerInParent="true"/>
    <TextView
        android:id="@+id/tv_empty"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="No Avaliable Data"
        android:layout_below="@+id/iv_empty"
        android:layout_centerHorizontal="true"/>
</com.example.waiyan.mmhealthkotlin.viewpods.EmptyViewPod>